\u0438\u0435 \u043F\u0440\u0438\u0435\u043C\u0443 \u0434\u043E\u043A\u0443\u043C\u0435\u043D\u0442\u043E\u0432, \u043C\u043E\u0433\u0443\u0442 \u0431\u044B\u0442\u044C\n\u0443\u0441\u0442\u0440\u0430\u043D\u0435\u043D\u044B \u0432 \u0445\u043E\u0434\u0435 \u043F\u0440\u0438\u0435\u043C\u0430, \u043E\u043D\u0438 \u0443\u0441\u0442\u0440\u0430\u043D\u044F\u044E\u0442\u0441\u044F \u043D\u0435\u0437\u0430\u043C\u0435\u0434\u043B\u0438\u0442\u0435\u043B\u044C\u043D\u043E.</p>\n\n<p class=\"MsoNormal\">\u0412 \u0441\u043B\u0443\u0447\u0430\u0435 \u043D\u0435\u0432\u043E\u0437\u043C\u043E\u0436\u043D\u043E\u0441\u0442\u0438 \u043D\u0435\u0437\u0430\u043C\u0435\u0434\u043B\u0438\u0442\u0435\u043B\u044C\u043D\u043E\u0433\u043E \u0443\u0441\u0442\u0440\u0430\u043D\u0435\u043D\u0438\u044F\n\u043F\u0440\u0435\u043F\u044F\u0442\u0441\u0442\u0432\u0438\u0439 \u0441\u043F\u0435\u0446\u0438\u0430\u043B\u0438\u0441\u0442 \u043F\u043E \u0441\u043E\u0433\u043B\u0430\u0441\u043E\u0432\u0430\u043D\u0438\u044E \u0441 \u0437\u0430\u044F\u0432\u0438\u0442\u0435\u043B\u0435\u043C \u043D\u0430\u0437\u043D\u0430\u0447\u0430\u0435\u0442 \u0432\u0440\u0435\u043C\u044F \u0441\u043B\u0435\u0434\u0443\u044E\u0449\u0435\u0433\u043E\n\u043F\u0440\u0438\u0435\u043C\u0430.</p>\n\n<p class=\"MsoNormal\">\u041F\u0440\u0438 \u043D\u0435\u043F\u043E\u0441\u0440\u0435\u0434\u0441\u0442\u0432\u0435\u043D\u043D\u043E\u043C \u043B\u0438\u0447\u043D\u043E\u043C \u043E\u0431\u0440\u0430\u0449\u0435\u043D\u0438\u0438 \u0437\u0430\u044F\u0432\u0438\u0442\u0435\u043B\u044F (\u0442\u0435\u043B\u0435\u0444\u043E\u043D\u043D\u043E\u043C\n\u0437\u0432\u043E\u043D\u043A\u0435) \u043E\u0442\u0432\u0435\u0442 (\u043A\u043E\u043D\u0441\u0443\u043B\u044C\u0442\u0430\u0446\u0438\u044F) \u0434\u0430\u0435\u0442\u0441\u044F \u0432 \u0443\u0441\u0442\u043D\u043E\u0439 \u0444\u043E\u0440\u043C\u0435 \u0441\u043F\u0435\u0446\u0438\u0430\u043B\u0438\u0441\u0442\u043E\u043C \u043E\u0442\u0434\u0435\u043B\u0430, \u0432\n\u0434\u043E\u043B\u0436\u043D\u043E\u0441\u0442\u043D\u044B\u0435 \u043E\u0431\u044F\u0437\u0430\u043D\u043D\u043E\u0441\u0442\u0438 \u043A\u043E\u0442\u043E\u0440\u043E\u0433\u043E \u0432\u0445\u043E\u0434\u0438\u0442 \u0432\u0435\u0434\u0435\u043D\u0438\u0435 \u043F\u043E\u0441\u0442\u0430\u0432\u043B\u0435\u043D\u043D\u043E\u0433\u043E \u0432\u043E\u043F\u0440\u043E\u0441\u0430.</p>\n\n<p class=\"MsoNormal\">\u041E\u0441\u043D\u043E\u0432\u0430\u043D\u0438\u0435\u043C \u0434\u043B\u044F \u043D\u0430\u0447\u0430\u043B\u0430 \u043F\u0440\u043E\u0446\u0435\u0434\u0443\u0440\u044B \u044F\u0432\u043B\u044F\u0435\u0442\u0441\u044F \u043F\u0438\u0441\u044C\u043C\u0435\u043D\u043D\u043E\u0435\n\u0437\u0430\u044F\u0432\u043B\u0435\u043D\u0438\u0435, \u043F\u0435\u0440\u0435\u0434\u0430\u043D\u043D\u043E\u0435 \u0438\u0441\u043F\u043E\u043B\u043D\u0438\u0442\u0435\u043B\u044E \u0432 \u043F\u043E\u0440\u044F\u0434\u043A\u0435 \u0434\u0435\u043B\u043E\u043F\u0440\u043E\u0438\u0437\u0432\u043E\u0434\u0441\u0442\u0432\u0430.</p>\n\n<p class=\"MsoNormal\">\u0418\u0441\u043F\u043E\u043B\u043D\u0438\u0442\u0435\u043B\u044C \u0440\u0430\u0441\u0441\u043C\u0430\u0442\u0440\u0438\u0432\u0430\u0435\u0442 \u043F\u043E\u0441\u0442\u0443\u043F\u0438\u0432\u0448\u0435\u0435 \u043F\u0438\u0441\u044C\u043C\u0435\u043D\u043D\u043E\u0435 \u0437\u0430\u044F\u0432\u043B\u0435\u043D\u0438\u0435 \u0432\n\u0441\u043B\u0435\u0434\u0443\u044E\u0449\u0435\u043C \u043F\u043E\u0440\u044F\u0434\u043A\u0435:</p>\n\n<p class=\"MsoNormal\">- \u0438\u0437\u0443\u0447\u0430\u0435\u0442 \u0437\u0430\u044F\u0432\u043B\u0435\u043D\u0438\u0435 \u0438 \u0433\u043E\u0442\u043E\u0432\u0438\u0442 \u043F\u0440\u043E\u0435\u043A\u0442 \u043E\u0442\u0432\u0435\u0442\u0430 \u043F\u043E \u0441\u0443\u0449\u0435\u0441\u0442\u0432\u0443\n\u043F\u043E\u0441\u0442\u0430\u0432\u043B\u0435\u043D\u043D\u044B\u0445 \u0432\u043E\u043F\u0440\u043E\u0441\u043E\u0432;</p>\n\n<p class=\"MsoNormal\">- \u043F\u0440\u043E\u0435\u043A\u0442 \u043E\u0442\u0432\u0435\u0442\u0430 \u043D\u0430\u043F\u0440\u0430\u0432\u043B\u044F\u0435\u0442\u0441\u044F \u043D\u0430 \u043F\u043E\u0434\u043F\u0438\u0441\u044C \u043D\u0430\u0447\u0430\u043B\u044C\u043D\u0438\u043A\u0443\n\u0423\u043F\u0440\u0430\u0432\u043B\u0435\u043D\u0438\u044F, \u0441\u043E\u0433\u043B\u0430\u0441\u043D\u043E \u0441\u0440\u043E\u043A\u043E\u0432 \u0443\u043A\u0430\u0437\u0430\u043D\u043D\u044B\u0445 \u0432 <a href=\"#sub_24\"><span class=\"a\"><span style=\"font-family: Arial; color: windowtext; text-decoration: none;\">\u043F. 2.4</span></span></a>. \u043D\u0430\u0441\u0442\u043E\u044F\u0449\u0435\u0433\u043E\n\u0440\u0435\u0433\u043B\u0430\u043C\u0435\u043D\u0442\u0430;</p>\n\n<p class=\"MsoNormal\"><a name=\"sub_31413\">- \u0441\u043F\u0435\u0446\u0438\u0430\u043B\u0438\u0441\u0442 \u043E\u0442\u0434\u0435\u043B\u0430, \u0443\u043F\u043E\u043B\u043D\u043E\u043C\u043E\u0447\u0435\u043D\u043D\u044B\u0439 \u043D\u0430\n\u043F\u0440\u0438\u0435\u043C \u0432\u0445\u043E\u0434\u044F\u0449\u0435\u0439 \u043A\u043E\u0440\u0440\u0435\u0441\u043F\u043E\u043D\u0434\u0435\u043D\u0446\u0438\u0438 \u0432 \u0441\u0438\u0441\u0442\u0435\u043C\u0435 \u043A\u043E\u043D\u0442\u0440\u043E\u043B\u044F \u0438 \u043F\u043E\u0441\u0442\u0443\u043F\u043B\u0435\u043D\u0438\u044F \u0434\u043E\u043A\u0443\u043C\u0435\u043D\u0442\u043E\u0432,\n\u043D\u0430\u043F\u0440\u0430\u0432\u043B\u044F\u0435\u0442 \u0432 \u0442\u0435\u0447\u0435\u043D\u0438\u0435 \u043F\u044F\u0442\u0438 \u0440\u0430\u0431\u043E\u0447\u0438\u0445 \u0434\u043D\u0435\u0439 \u043E\u0442\u0432\u0435\u0442 \u0437\u0430\u044F\u0432\u0438\u0442\u0435\u043B\u044E, \u043E \u0447\u0435\u043C \u0434\u0435\u043B\u0430\u0435\u0442 \u0437\u0430\u043F\u0438\u0441\u044C \u0432\n\u043A\u043D\u0438\u0433\u0435 \u0438\u0441\u0445\u043E\u0434\u044F\u0449\u0438\u0445 \u0434\u043E\u043A\u0443\u043C\u0435\u043D\u0442\u043E\u0432 \u0423\u043F\u0440\u0430\u0432\u043B\u0435\u043D\u0438\u044F.</a></p>\n\n<span style=\"\"></span>\n\n<p class=\"MsoNormal\">\u041F\u0440\u0438 \u043D\u0435\u043F\u043E\u0441\u0440\u0435\u0434\u0441\u0442\u0432\u0435\u043D\u043D\u043E\u043C \u043E\u0431\u0440\u0430\u0449\u0435\u043D\u0438\u0438 \u0437\u0430\u044F\u0432\u0438\u0442\u0435\u043B\u044F, \u0432 \u0442.\u0447. \u043F\u043E\n\u0442\u0435\u043B\u0435\u0444\u043E\u043D\u0443, \u043E\u0442\u0432\u0435\u0442 (\u043A\u043E\u043D\u0441\u0443\u043B\u044C\u0442\u0430\u0446\u0438\u044F) \u0434\u0430\u0435\u0442\u0441\u044F \u0432 \u0443\u0441\u0442\u043D\u043E\u0439 \u0444\u043E\u0440\u043C\u0435 \u0441\u043F\u0435\u0446\u0438\u0430\u043B\u0438\u0441\u0442\u043E\u043C \u043E\u0442\u0434\u0435\u043B\u0430, \u0432\n\u043F\u0440\u0435\u0434\u0435\u043B\u0430\u0445 \u0441\u0432\u043E\u0435\u0439 \u043A\u043E\u043C\u043F\u0435\u0442\u0435\u043D\u0446\u0438\u0438.</p>\n\n<p class=\"MsoNormal\">\u041F\u0440\u0438\u0435\u043C \u0437\u0430\u044F\u0432\u043B\u0435\u043D\u0438\u0439 \u0438 \u043F\u0440\u0438\u043B\u0430\u0433\u0430\u0435\u043C\u044B\u0445 \u0434\u043E\u043A\u0443\u043C\u0435\u043D\u0442\u043E\u0432, \u043F\u043E\u0441\u0442\u0443\u043F\u0438\u0432\u0448\u0438\u0445 \u0432\n\u044D\u043B\u0435\u043A\u0442\u0440\u043E\u043D\u043D\u043E\u0439 \u0444\u043E\u0440\u043C\u0435 \u0447\u0435\u0440\u0435\u0437 \u043F\u043E\u0440\u0442\u0430\u043B \u0413\u0443\u0431\u0435\u0440\u043D\u0430\u0442\u043E\u0440\u0430 \u0438 \u0410\u0434\u043C\u0438\u043D\u0438\u0441\u0442\u0440\u0430\u0446\u0438\u0438 \u0412\u043E\u043B\u0433\u043E\u0433\u0440\u0430\u0434\u0441\u043A\u043E\u0439\n\u043E\u0431\u043B\u0430\u0441\u0442\u0438 \u0432 \u0438\u043D\u0444\u043E\u0440\u043C\u0430\u0446\u0438\u043E\u043D\u043D\u043E \u2013 \u0442\u0435\u043B\u0435\u043A\u043E\u043C\u043C\u0443\u043D\u0438\u043A\u0430\u0446\u0438\u043E\u043D\u043D\u043E\u0439 \u0441\u0435\u0442\u0438 \u0418\u043D\u0442\u0435\u0440\u043D\u0435\u0442, \u043E\u0441\u0443\u0449\u0435\u0441\u0442\u0432\u043B\u044F\u0435\u0442\u0441\u044F\n\u0441\u043E\u0442\u0440\